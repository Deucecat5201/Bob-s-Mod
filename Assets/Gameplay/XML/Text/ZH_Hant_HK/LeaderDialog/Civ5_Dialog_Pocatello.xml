    <Row Tag="TXT_KEY_LEADER_POCATELLO_AGREE_SHORT_1">
      <Text>好吧。</Text>
    </Row>
    <Row Tag="TXT_KEY_LEADER_POCATELLO_AGREE_SHORT_2">
      <Text>很好。</Text>
    </Row>
    <Row Tag="TXT_KEY_LEADER_POCATELLO_AGREE_SHORT_3">
